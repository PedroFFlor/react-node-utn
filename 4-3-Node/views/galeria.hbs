<main class="container-sm text-center">
   <div class="row justify-content-evenly">
      <h3>Rubro 1</h3>
      {{#each productos}}     
         <div class="col-3 d-flex flex-column">
            <span>{{this.nombreProducto}}</span>
            <img src={{this.imagen}} alt="Producto 1">
            <h6>Embalaje:<stong>{{this.embalaje}}</stong>
            </h6>
            <h3>{{this.precio}}</h3>
         </div>
      {{/each}}
   </div>
</main>